{
    "runs": 100,
    "targets": [
      {
        "name": "es1",
        "type": "elasticsearch",
        "config": {
          "cluster": "host.docker.internal",
          "indices": "syslog-2021-02-24,syslog-2021-02-25"
        }
      },
      {
        "name": "clickhouse1",
        "type": "clickhouse",
        "config": {
          "cluster": "host.docker.internal"
        }
      }
    ],
    "testcases": [
      [
        "case1",
        "query all",
        [
          {
            "query": {
              "match_all": {}
            }
          },
          "SELECT raw FROM syslog"
        ]
      ],
      [
        "case2",
        "match all",
        [
          {
            "query": {
              "match": {
                "hostname": "for.org"
              }
            }
          },
          "SELECT raw FROM syslog WHERE hostname='for.org'"
        ]
      ],
      [
        "case3",
        "multi match",
        [
          {
            "query": {
              "multi_match": {
                "query": "up.com ahmadajmi",
                "fields": [
                  "hostname",
                  "application"
                ]
              }
            }
          },
          "SELECT raw FROM syslog WHERE hostname='for.org' OR application='ahmadajmi'"
        ]
      ],
      [
        "case4",
        "term query",
        [
          {
            "query": {
              "term": {
                "message": "pretty"
              }
            }
          },
          "SELECT raw FROM syslog WHERE lowerUTF8(raw) LIKE '%pretty%'"
        ]
      ],
      [
        "case5",
        "range query",
        [
          {
            "query": {
              "range": {
                "version": {
                  "gte": 2
                }
              }
            }
          },
          "SELECT raw FROM syslog WHERE version >= 2"
        ]
      ]
    ]
  }