{
    "runs": 100,
    "targets": [
      {
        "name": "es1",
        "type": "elasticsearch",
        "config": {
          "cluster": "host.docker.internal",
          "indices": "syslog-2021-02-24,syslog-2021-02-25"
        }
      },
      {
        "name": "clickhouse1",
        "type": "clickhouse",
        "config": {
          "cluster": "host.docker.internal"
        }
      }
    ],
    "testcases": [
      [
        "case1",
        "query all",
        [
          {
            "query": {
              "match_all": {}
            }
          },
          "SELECT raw FROM syslog"
        ]
      ],
      [
        "case2",
        "query all",
        [
          {
            "query": {
              "match": {
                "hostname": "for.org"
              }
            }
          },
          "SELECT raw FROM syslog WHERE hostname='for.org'"
        ]
      ]
    ]
  }