{
    "runs": 10,
    "targets": [
      {
        "name": "es1",
        "type": "elasticsearch",
        "config": {
          "cluster": "host.docker.internal",
          "indices": "syslog-2021-02-24,syslog-2021-02-25,syslog-2021-02-26"
        }
      },
      {
        "name": "clickhouse1",
        "type": "clickhouse",
        "config": {
          "cluster": "host.docker.internal"
        }
      }
    ],
    "testcases": [
      [
        "case1",
        "query all",
        [
          {
            "query": {
              "match_all": {}
            }
          },
          "SELECT * FROM syslog"
        ]
      ],
      [
        "case2",
        "match all",
        [
          {
            "query": {
              "match": {
                "hostname": "for.org"
              }
            }
          },
          "SELECT * FROM syslog WHERE hostname='for.org'"
        ]
      ],
      [
        "case3",
        "multi match",
        [
          {
            "query": {
              "multi_match": {
                "query": "up.com ahmadajmi",
                "fields": [
                  "hostname",
                  "application"
                ]
              }
            }
          },
          "SELECT * FROM syslog WHERE hostname='for.org' OR application='ahmadajmi'"
        ]
      ],
      [
        "case4",
        "term query",
        [
          {
            "query": {
              "term": {
                "message": "pretty"
              }
            }
          },
          "SELECT * FROM syslog WHERE lowerUTF8(raw) LIKE '%pretty%'"
        ]
      ],
      [
        "case5",
        "range query",
        [
          {
            "query": {
              "range": {
                "version": {
                  "gte": 2
                }
              }
            }
          },
          "SELECT * FROM syslog WHERE version >= 2"
        ]
      ],
      [
        "case6",
        "exist query",
        [
          {
            "query": {
              "exists": {
                "field": "application"
              }
            }
          },
          "SELECT * FROM syslog WHERE application is not NULL"
        ]
      ],
      [
        "case7",
        "regex query",
        [
          {
            "query": {
              "regexp": {
                "message": {
                  "value": "good*",
                  "flags": "ALL",
                  "max_determinized_states": 10000,
                  "rewrite": "constant_score"
                }
              }
            }
          },
          "SELECT * FROM syslog WHERE match(hostname, 'up.*')"
        ]
      ]
    ]
  }